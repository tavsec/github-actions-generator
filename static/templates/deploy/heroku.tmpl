{{=[[ ]]=}}
deploy:
    needs: build
    runs-on: ubuntu-latest

    steps:
        - name: Deploy to Heroku
        uses: akhileshns/heroku-deploy@v3.12.12
        with:
            heroku_api_key: ${{ secrets.HEROKU_API_KEY }}
            heroku_app_name: [[herokuAppName]]
            heroku_email: [[herokuEmail]]